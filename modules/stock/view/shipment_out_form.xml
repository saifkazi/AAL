<form cursor="customer">
    <label name="reference"/>
    <field name="reference"/>
    <label name="challan_no"/>
    <field name="challan_no"/>
    <label name="number"/>
    <field name="number"/>
    <label name="customer"/>
    <field name="customer"/>
    <label name="delivery_address"/>
    <field name="delivery_address"/>
    <label name="planned_date"/>
    <field name="planned_date"/>
    <label name="effective_date"/>
    <field name="effective_date"/>
    <label name="company"/>
    <field name="company"/>
    <label name="warehouse"/>
    <field name="warehouse"/>
    <label name="bulk"/>
    <field name="bulk" width="10"  colspan="1"/>
    <label name="vehicle_no"/>
    <field name="vehicle_no"/>
    <label name="transporter_name"/>
    <field name="transporter_name"/>
    <group col="-1" colspan="2" id="bulks">
        <label name="ms"/>
        <field name="ms" xexpand="0" width="25"/>
         <label name="gi"/>
        <field name="gi" xexpand="0" width="25"/>
         <label name="pvc"/>
        <field name="pvc" xexpand="0" width="25"/>
         <label name="l1000"/>
        <field name="l1000" xexpand="0" width="25"/>
        <!-- Add here some checkboxes ! -->
    </group>
    <label name="gross_wt"/>
    <field name="gross_wt"/>
    <label name="tare_wt"/>
    <field name="tare_wt"/>
    <label name="net_wt"/>
    <field name="net_wt"/>
    <notebook colspan="4">
        <page name="inventory_moves">
            <field name="inventory_moves" colspan="4" view_ids="stock.move_view_list_shipment"/>
        </page>
        <page string="Outgoing Moves" id="outgoing_moves">
            <field name="outgoing_moves" colspan="4" view_ids="stock.move_view_list_shipment"/>
        </page>
        <page string="Other Info" id="other">
            <label name="picked_by"/>
            <field name="picked_by"/>
            <label name="packed_by"/>
            <field name="packed_by"/>
            <label name="done_by"/>
            <field name="done_by"/>
        </page>
    </notebook>
    <label name="state"/>
    <field name="state"/>
    <group col="-1" colspan="2" id="buttons">
        <button name="cancel" icon="tryton-cancel"/>
        <button name="draft"/>
        <button name="wait"/>
        <button name="assign_wizard" icon="tryton-forward"/>
        <button name="pick" icon="tryton-forward"/>
        <button name="pack" icon="tryton-forward"/>
        <button name="done" icon="tryton-ok"/>
    </group>
</form>
